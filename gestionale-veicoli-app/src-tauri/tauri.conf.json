{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "Gestionale Veicoli",
  "version": "0.1.0",
  "identifier": "com.gestionaleveicoli",
  "build": {
    "beforeBuildCommand": "npm run build",
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../dist"
  },
  "app": {
    "security": {
      "csp": null,
      "capabilities": [
        "allow-print"
      ]
    },
    "windows": [
      {
        "title": "Gestionale Veicoli • Scozzarini Service Car • dev by Haxies",
        "width": 1280,
        "height": 800,
        "resizable": true
      }
    ]
  },
  "bundle": {
    "active": true,
    "targets": "all"
  },
  "plugins": {}
}

